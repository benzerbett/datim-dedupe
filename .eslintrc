{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jasmine": true,
    "jquery": true
  },
  "globals": {
    "inject": false,
    "angular": false,
    "_": false
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "wrap-iife": 2,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": 0,
    "max-depth": [
      2,
      5
    ],
    "max-statements": [
      2,
      45
    ],
    "complexity": [
      2,
      10
    ],
    "max-len": [
      2,
      200
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "no-multi-str": 2,
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": true
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "no-multiple-empty-lines": 2,
    "keyword-spacing": [
      2,
      {}
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "no-underscore-dangle": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-infix-ops": 2,
    "no-with": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "eol-last": 2,
    "dot-notation": 2,
    "yoda": [
      2,
      "never"
    ]
  }
}
